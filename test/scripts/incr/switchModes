#!/bin/sh

STARTTIME=`date "+%s"`
echo 'Starting switchModes: ' `date`

set -e

# Start: FormView/CodeView js-editor ------

# first build it from scratch
scc -a -ta editor/js/mixin example/unitConverter/jsui < test/scripts/wait10.txt

scc -ta editor/js/mixin example/unitConverter/jsui < test/scripts/wait10.txt

scc -a -ta -dyn editor/js/mixin example/unitConverter/jsui < test/scripts/wait20.txt

scc -ta -dyn editor/js/mixin example/unitConverter/jsui < test/scripts/wait20.txt

scc -ta editor/js/mixin example/unitConverter/jsui < test/scripts/wait10.txt
scc -ta -dyn editor/js/mixin example/unitConverter/jsui < test/scripts/wait20.txt

scc -ta editor/js/mixin example/unitConverter/jsui < test/scripts/wait10.txt
scc -ta -dyn editor/js/mixin example/unitConverter/jsui < test/scripts/wait20.txt

scc -ta -ee editor/js/mixin example/unitConverter/jsui < test/scripts/wait10.txt
scc -ta -ee -dyn editor/js/mixin example/unitConverter/jsui < test/scripts/wait20.txt


# ------

ENDTIME=`date "+%s"`
RUNTIME=`expr $ENDTIME - $STARTTIME`

echo 'Run time: ' `date -j -f "%s" $RUNTIME "+%M:%S"`
echo 'Ending time: ' `date` 
