(cd test/refreshMQ; ./run)

sc -a test/emptySwing -i < example/unitConverter/test/buildScript.txt -r sc.swing.Main

#  Annoying tests that pop up lots of windows

(cd test/wicketUC; ./run)

(cd test/wicketQuiz; ./run)

(cd test/wicketMQ; ./run)

