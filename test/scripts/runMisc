#!/bin/sh

set -e

sc -ni -a -ta test/ifaceField/ibase
sc -ni -a -ta test/ifaceField/ibind

sc -ni -a test/componentInner

sc -ni -dynone -a -ta test/ifaceField/ibase
sc -ni -dynone -a -ta test/ifaceField/ibind

