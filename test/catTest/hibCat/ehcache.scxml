<ehcache>

    <defaultCache
            maxElementsInMemory="<%= CacheTest.itemCacheSize%>"
            eternal="true"
            timeToIdleSeconds="12000"
            timeToLiveSeconds="12000"
            overflowToDisk="false"
            diskPersistent="false"
            diskExpiryThreadIntervalSeconds="12000"
            memoryStoreEvictionPolicy="LRU"/>

</ehcache>
