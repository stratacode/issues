<%! object oldIndex extends util { 
   String loadScripts() { %>
      <script src="js/jquery.js" type="text/javascript"></script>
      <script src="js/jquery.cycle.all.js" type="text/javascript"></script>
      <script type="text/javascript">

     function initCycles() {
        $('.innerSlides').cycle().cycle('stop');
        var currentIndex = 0;
        var timeOuts = [5000, 9000, 24000, 30000, 12500, 5000, 5000];

         $('#mainSlides').cycle({
             fx:    'scrollLeft', 
             next:  '#mainnext',
             prev:  '#mainprev',
             pager:  '#mainpager',
             timeoutFn: function(currElement, nextElement, opts, isForward) { 
                 var index = opts.currSlide;
                 return index < timeOuts.length ? timeOuts[index] : 3000;
             },
             pagerAnchorBuilder: function(idx, slide) {
                    return '<li>';
             },
             before: function(o1,o2,theCycle,o4) {
                  // stop all inner slideshows
                 $('.innerSlides').cycle('stop');
                 currentIndex = theCycle.nextSlide; 
                 var theName = "#slide" + currentIndex  + "Inner";

                 var theElem = $(theName).get(0);
                 if (typeof theElem !== 'undefined' && 
                     typeof theElem.cycleInitialized === 'undefined') {
                     theElem.cycleInitialized = "true";
                     // start the new slide's slideshow
                     $(theName).cycle({
                         fx:    'scrollLeft', 
                         timeout: 6000,
                         next: "#innernext" + currentIndex,
                         prev: "#innerprev" + currentIndex,
                         pager: "#innerpager" + currentIndex,
                         pagerAnchorBuilder: function(idx, slide) {
                                return '<li class="innerPagerLi">';
                         }
                     });
                     $('.innerPagerLi').click(function() { 
                             $('#mainSlides').cycle('pause'); 
                             $('.innerSlides').cycle('pause'); 
                         });
                 }
                 else {
                    $(theName).cycle().cycle('resume');
                 }
             }
         });
 
         $('#mainpager li').click(function() { 
                 $('#mainSlides').cycle('pause'); 
                 $('.innerSlides').cycle('pause'); 
             });
         $('.pagearrow').click(function() { 
                 $('#mainSlides').cycle('pause'); 
                 $('.innerSlides').cycle('pause'); 
             });

       }

     $(document).ready(function() {
         initCycles();
     });

      </script>
<% } %>
